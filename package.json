{
  "name": "pwa-todos",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "build:dev": "npm run env -s && ng build --prod --optimization false --build-optimizer false --watch",
    "start": "npm run env -s && ng serve",
    "server": "http-server dist -c-1 --proxy http://localhost:8080?",
    "deploy": "npm run env -s && firebase deploy --only hosting:pwa-todos",
    "lint": "stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@angular/service-worker": "^11.0.4",
    "firebase": "^7.0 || ^8.0",
    "idb": "^5.0.8",
    "primeflex": "2.0.0",
    "primeicons": "4.0.0",
    "primeng": "10.0.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1200",
    "@angular-devkit/build-angular": "^0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@angular/language-service": "^10.1.4",
    "@angularclass/hmr": "^3.0.0",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/node": "^12.12.62",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "codelyzer": "^6.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "htmlhint": "^0.14.1",
    "husky": "^4.2.5",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "lint-staged": "^10.5.3",
    "open": "^7.0.3",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "stylelint": "~13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --fix --syntax=scss"
  }
}
